<div class="wrapper">
  <div class="menu-menu">
    <ul>
      <li>About<%= image_tag("arrow-up-right.svg", class:"arrow") %></li>
      <% @projects.each do |project| %>
      <li><%= project.name %><%= image_tag("arrow-up-right.svg", class:"arrow") %></li>
      <% end %>
    </ul>
  </div>

  <div class="projects">
    <ul>
      <% @projects.each do |project| %>
      <% project.pictures.each do |picture| %>
      <li><%= image_tag(picture.url) %></li>
      <% end %>
      <%= project.description %>
      <% end %>
    </ul>
  </div>
</div>

